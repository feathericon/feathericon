{
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
  },
  "rules": {
      "no-console": ["error", {
        "allow": ["log", "debug", "error"]
      }],
      "no-unused-vars": 1
  }
}
